package Fundamentos;

public class ConversaoTipoPrimitivoNumerico {
	public static void main(String[] args) {
		
		//Implícita
		double a = 1;
		System.out.println(a);
		//Explícita
		float b= (float) 1.123456789999;
		System.out.println(b);
		
		int c = 340;
		byte d = (byte) c;
		System.out.println(d);
	}

}
